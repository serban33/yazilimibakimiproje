<!DOCTYPE html>
<html class="no-js" lang="tr">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>ENG MERCEDES</title>

    <!--== Favicon ==-->
    <link rel="shortcut icon" href="/Content/assets/img/favicon.ico" type="image/x-icon" />

    <!--== Google Fonts ==-->
    <link href="https://fonts.googleapis.com/css?family=Oswald:400,500,600,700%7CPoppins:400,400i,500,600&display=swap" rel="stylesheet">

    <!-- build:css /Content/assets/css/app.min.css -->
    <!--== Leaflet Min CSS ==-->
    <link href="/Content/assets/css/leaflet.min.css" rel="stylesheet" />
    <!--== Nice Select Min CSS ==-->
    <link href="/Content/assets/css/nice-select.min.css" rel="stylesheet" />
    <!--== Slick Slider Min CSS ==-->
    <link href="/Content/assets/css/slick.min.css" rel="stylesheet" />
    <!--== Magnific Popup Min CSS ==-->
    <link href="/Content/assets/css/magnific-popup.min.css" rel="stylesheet" />
    <!--== Slicknav Min CSS ==-->
    <link href="/Content/assets/css/slicknav.min.css" rel="stylesheet" />
    <!--== Animate Min CSS ==-->
    <link href="/Content/assets/css/animate.min.css" rel="stylesheet" />
    <!--== Ionicons Min CSS ==-->
    <link href="/Content/assets/css/ionicons.min.css" rel="stylesheet" />
    <!--== Font-Awesome Min CSS ==-->
    <link href="/Content/assets/css/font-awesome.min.css" rel="stylesheet" />
    <!--== Bootstrap Min CSS ==-->
    <link href="/Content/assets/css/bootstrap.min.css" rel="stylesheet" />

    <!--== Main Style CSS ==-->
    <link href="/Content/assets/css/style.css" rel="stylesheet" />
    <!--== Helper Min CSS ==-->
    <link href="/Content/assets/css/helper.min.css" rel="stylesheet" />
    <!-- endbuild -->
    <!--[if lt IE 9]>
    <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <!--== Start Header Area ==-->
    <header class="header-area">
        <div class="container container-wide">
            <div class="row align-items-center">
                <div class="col-sm-4 col-lg-2">
                    <div class="site-logo text-center text-sm-left">
                        <a href="/"><img src="/Content/assets/img/Mercedes.jpg" alt="ENG MERCEDES" /></a>
                    </div>
                </div>

                <div class="col-lg-7 col-sm-8 col-8 d-none d-lg-block">
                    <div class="site-navigation">
                        <ul class="main-menu nav">
                            <li class="has-submenu">
                                <a href="/">Anasayfa</a>

                            </li>
                            <li><a href="/hakkimizda">Hakkımızda</a></li>
                            <li class="has-submenu">
                                <a href="/Urunler">Urunler</a>

                            </li>
                            <li class="has-submenu">
                                <a href="/Markalar">Markalar</a>

                            </li>
                            <li class="has-submenu">
                                <a href="/Iletisim">İletişim</a>

                            </li>
                        </ul>
                    </div>
                </div>

                <div class="col-sm-8 col-lg-3">
                    <div class="site-action d-flex justify-content-center justify-content-sm-end align-items-center">
                       

                        <div class="responsive-menu d-lg-none">
                            <button class="btn-menu">
                                <i class="fa fa-bars"></i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!--== End Header Area ==-->
    @RenderBody()
    <!--== End Brand Logo Area Wrapper ==-->
    <!--== Start Footer Area Wrapper ==-->
    <footer class="footer-area">
        <div class="footer-widget-area">
            <div class="container container-wide">
                <div class="row mtn-40">
                    <div class="col-lg-3">
                        <div class="widget-item">
                            <div class="about-widget">
                                <a href="/"><img src="/Content/assets/img/Mercedes.jpg" alt="Logo" /></a>
                                <p>
                                    @if (TempData["SiteAciklama"] != null)
                                    {
                                        @TempData["SiteAciklama"]
                                    }

                                </p>
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6 col-lg-2">
                        <div class="widget-item">
                            <h4 class="widget-title">Hakkımızda</h4>
                            <div class="widget-body">
                                <ul class="widget-list">
                                    <li><a href="/hakkimizda">Biz Kimiz?</a></li>
                                    <li><a href="/hakkimizda">Vizyonumuz</a></li>
                                    <li><a href="/hakkimizda">Misyonumuz</a></li>
                                    
                                    
                                </ul>
                            </div>
                        </div>
                    </div>

                 

                    <div class="col-sm-6 col-lg-2">
                        <div class="widget-item">
                            <h4 class="widget-title">Hızlı Erişim</h4>
                            <div class="widget-body">
                                <ul class="widget-list">
                                    <li><a href="/">Anasayfa</a></li>
                                    <li><a href="/hakkimizda">Hakkımızda</a></li>
                                    <li><a href="/urunler">Urunler</a></li>
                                    <li><a href="/markalar">Markalar</a></li>
                                    <li><a href="/iletisim">Iletişim</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6 col-lg-3">
                        <div class="widget-item">
                            <h4 class="widget-title">İletişim Bilgileri</h4>
                            <div class="widget-body">
                                <address>
                                    @TempData["SiteAdres"] <br>
                                    @TempData["SiteMail"] <br>
                                    @TempData["Telefon1"]
                                    @TempData["Telefon2"]
                                </address>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="footer-copyright-area">
            <div class="container">
                <div class="row">
                    <div class="col-12 text-center">
                        <div class="copyright-content">
                            <p>Copyright © 2020 ENG MERCEDES. All Rights Reserved.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!--== End Footer Area Wrapper ==-->
    <!-- Scroll Top Button -->
    <button class="btn-scroll-top"><i class="ion-chevron-up"></i></button>



    <!--== Start Responsive Menu Wrapper ==-->
    <aside class="off-canvas-wrapper off-canvas-menu">
        <div class="off-canvas-overlay"></div>
        <div class="off-canvas-inner">
            <!-- Start Off Canvas Content -->
            <div class="off-canvas-content">
                <div class="off-canvas-header">
                    <div class="logo">
                        <a href="/"><img src="/Content/assets/img/Mercedes.jpg" alt="Logo"></a>
                    </div>
                    <div class="close-btn">
                        <button class="btn-close"><i class="ion-android-close"></i></button>
                    </div>
                </div>

                <!-- Content Auto Generate Form Main Menu Here -->
                <div class="res-mobile-menu mobile-menu">

                </div>
            </div>
        </div>
    </aside>
    <!--== End Responsive Menu Wrapper ==-->
    <!--=======================Javascript============================-->
    <!-- build:js /Content/assets/js/app.min.js -->
    <!--=== Modernizr Min Js ===-->
    <script src="/Content/assets/js/modernizr-3.6.0.min.js"></script>
    <!--=== jQuery Min Js ===-->
    <script src="/Content/assets/js/jquery-3.3.1.min.js"></script>
    <!--=== jQuery Migration Min Js ===-->
    <script src="/Content/assets/js/jquery-migrate-1.2.1.min.js"></script>
    <!--=== Popper Min Js ===-->
    <script src="/Content/assets/js/popper.min.js"></script>
    <!--=== Bootstrap Min Js ===-->
    <script src="/Content/assets/js/bootstrap.min.js"></script>
    <!--=== Slicknav Min Js ===-->
    <script src="/Content/assets/js/jquery.slicknav.min.js"></script>
    <!--=== Magnific Popup Min Js ===-->
    <script src="/Content/assets/js/jquery.magnific-popup.min.js"></script>
    <!--=== Slick Slider Min Js ===-->
    <script src="/Content/assets/js/slick.min.js"></script>
    <!--=== Nice Select Min Js ===-->
    <script src="/Content/assets/js/jquery.nice-select.min.js"></script>
    <!--=== Leaflet Min Js ===-->
    <script src="/Content/assets/js/leaflet.min.js"></script>
    <!--=== Countdown Js ===-->
    <script src="/Content/assets/js/countdown.js"></script>

    <!--=== Active Js ===-->
    <script src="/Content/assets/js/active.js"></script>
    <!-- endbuild -->

</body>

</html>